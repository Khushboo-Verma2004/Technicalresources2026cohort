import java.util.*;
public class a5_q5 {
    public static void main(String[] args) {
        try (// write your code here  
        Scanner scn = new Scanner(System.in)) {
            int n = scn.nextInt();
            while(n>0){
                int x = n % 10;
                n= n/10;
                System.out.println(x);
            }
        }
       }
      }
    
